{
  "name": "@sanny-io/steamlogin",
  "version": "1.0.2",
  "description": "Dead simple functions for logging in through Steam.",
  "repository": "github:sanny-io/steamlogin",
  "homepage": "https://github.com/sanny-io/steamlogin",
  "bugs": "https://github.com/sanny-io/steamlogin/issues",
  "main": "./lib/cjs/index.js",
  "module": "./lib/mjs/index.js",
  "exports": {
    ".": {
      "import": "./lib/mjs/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./client": {
      "import": "./lib/mjs/client.js"
    },
    "./server": {
      "import": "./lib/mjs/server.js",
      "require": "./lib/cjs/server.js"
    }
  },
  "scripts": {
    "build": "rm -rf lib && tsc --project tsconfig.mjs.json && tsc --project tsconfig.cjs.json && node scripts/write-package-json.js",
    "prepare": "npm run build"
  },
  "author": "sanny-io",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}